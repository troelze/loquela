{{> navbar}}
<div class="container ">
    <h1 class="text-center pt-5">Users</h1>
    <table class="table">
        <thead> 
            <th>ID</th> 
            <th>Username</th> 
            <th>Email</th>
        </thead> 
        <tbody> 
            {{#each allUsers}} 
            <tr> 
                <td>{{id}}</td> 
                <td>{{username}}</td> 
                <td>{{email}}</td> 
            </tr> 
            {{/each}} 
        </tbody> 
    </table>  
</div>
